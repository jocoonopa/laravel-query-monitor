{
    "name": "jocoonopa/laravel-query-monitor",
    "type": "library",
    "description": "Laravel Query Monitor",
    "keywords": [
        "laravel", "eloquent", "query", "sql", "monitor"
    ],
    "authors": [
        {
            "name": "Jocoonopa",
            "email": "jocoonopa@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jocoonopa\\LaravelQueryMonitor\\": "src/"
        }
    },
    "require": {
        "laravel/framework": "^7.0 || ^8.0",
        "php": "^7.3 || ^8.0",
        "react/socket": "^1.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Jocoonopa\\LaravelQueryMonitor\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.2@dev",
        "orchestra/testbench": "^3.6 || ^4.0 || ^5.0 || ^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
